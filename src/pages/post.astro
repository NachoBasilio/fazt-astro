---
interface User {
  userId: number;
  id: number;
  title: string;
  body: string;
}
const response = await fetch("https://jsonplaceholder.typicode.com/posts")
const data = await response.json()
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    {
        data.map((user:User) =>{
            return(
                <div>
                    <h1>{user.title}</h1>
                    <p>{user.body}</p>
                </div>
            )
        })
    }
</body>
</html>